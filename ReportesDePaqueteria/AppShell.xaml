<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ReportesDePaqueteria.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ReportesDePaqueteria"
    xmlns:views="clr-namespace:ReportesDePaqueteria.MVVVM.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="ReportesDePaqueteria">

    <ShellContent
        Title="Sign In"
        ContentTemplate="{DataTemplate views:SignInView}"
        Route="SignIn" />

    <ShellContent
        Title="Sign Up"
        ContentTemplate="{DataTemplate views:SignUpView}"
        Route="SignUp" />

</Shell>
